# Capacitor Environment Variables
# 複製這個檔案為 .env 並填入你的設定值

# 應用程式基本資訊
APP_NAME=Hello World App
APP_ID=com.example.helloworldapp
APP_VERSION=1.0.0

# 開發設定
DEV_SERVER_PORT=8080
DEV_SERVER_HOST=localhost

# iOS 設定
IOS_BUNDLE_ID=com.example.helloworldapp
IOS_TEAM_ID=YOUR_TEAM_ID_HERE

# 建構設定  
BUILD_MODE=development